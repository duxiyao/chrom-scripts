{
  "manifest_version": 3,
  "name": "yy_work",
  "version": "0.0.1",
  "description": "yy_work",
  "author": "tuntun",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "*://mms.pinduoduo.com/*"
      ],
      "js": [
        "content-script.js"
      ],
      "css": [
        "insert-css.css"
      ]
    }
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "permissions": [
    "scripting",
    "tabs",
    "activeTab",
    "cookies",
    "storage",
    "debugger"
  ],
  "action": {
    "default_icon": {
      "16": "img/icon.png",
      "32": "img/icon.png",
      "48": "img/icon.png",
      "64": "img/icon.png",
      "128": "img/icon.png"
    }
  },
  "icons": {
    "16": "img/icon.png",
    "32": "img/icon.png",
    "48": "img/icon.png",
    "64": "img/icon.png",
    "128": "img/icon.png"
  }
}